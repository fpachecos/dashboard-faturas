{
  "cli": {
    "version": ">= 5.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "image": "latest",
        "xcode": "16.0"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false,
        "image": "latest",
        "xcode": "16.0"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

