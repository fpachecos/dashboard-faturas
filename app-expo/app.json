{
  "expo": {
    "name": "Dashboard Faturas",
    "slug": "dashboard-faturas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fpachecosouza.dashboardfaturas",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fpachecosouza.dashboardfaturas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "dashboardfaturas",
    "extra": {
      "eas": {
        "projectId": "a9d3a850-5a17-4dd0-8468-c1520e86c858"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "fpachecosouza"
  }
}
